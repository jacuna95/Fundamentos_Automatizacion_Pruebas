<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Account Tests">
    <test name="login_Test- Chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="testAccount"/>
        </classes>
    </test>
    <test name="Search Tests - Firefox">
        <parameter name="browser" value="firefox"></parameter>
        <parameter name="searchCriteria" value="iphone"></parameter>
        <parameter name="expectedResults" value="1"></parameter>
        <classes>
            <class name="TestSearch"/>
        </classes>
    </test>
</suite>